<AuthoringTool>NASA/maestro</AuthoringTool>
<MetaData procType="Nominal" status="FIN">
    <Date>{{date}}</Date>
    <UniqueId>{{uniqueId}}</UniqueId>
    <Book>{{book}}</Book>
    <Applicability>{{applicability}}</Applicability>
    <Version>{{version}}</Version>
    <ProcCode>{{procCode}}</ProcCode>
</MetaData>
<ProcTitle>
    <ProcNumber>{{number|default("X.X.XXX")}}</ProcNumber>
    <Text>{{name}}</Text>
</ProcTitle>
<TimeRequirement>
    <?xm-replace_text {Time Requirement}?>
</TimeRequirement>
<ProcedureObjective>
    <Para>
        <Text>{{objective}}</Text>
    </Para>
</ProcedureObjective>
<ItemizedList>
    <ListTitle>
        <Text>LOCATION:</Text>
    </ListTitle>
    {% for line in location %}
    <Para>
        <text>{{line}}</text>
    </Para>
    {% endfor %}
</ItemizedList>
<ItemizedList>
    <ListTitle>
        <Text>DURATION:</Text>
    </ListTitle>
    {% for line in duration %}
    <Para>
        <Text>{{line}}</Text>
    </Para>
    {% endfor %}
</ItemizedList>
<ItemizedList>
    <ListTitle>
        <Text>CREW:</Text>
    </ListTitle>
    {% for line in crewRequired %}
    <Para>
        <Text>{{line}}</Text>
    </Para>
    {% endfor %}
</ItemizedList>
<ToolsPartsMaterials>
    <Parts>
    {% for part in parts %}
        <ToolsItem toolId="it1">
            <ToolsItemName>
                <Text>{{parts[loop.index0].toolName}}</Text>
            </ToolsItemName>
            <PartNumber>
                <Text>{{parts[loop.index0].partNumber}}</Text>
            </PartNumber>
        </ToolsItem>
    {% endfor %}
    </Parts>
    <Materials>
    {% for material in materials %}
        <ToolsItem toolId="it1">
            <ToolsItemName>
                <Text>{{materials[loop.index0].toolName}}</Text>
            </ToolsItemName>
            <PartNumber>
                <Text>{{materials[loop.index0].partNumber}}</Text>
            </PartNumber>
        </ToolsItem>
    {% endfor %}
    </Materials>
    <Tools>
        {{tools}}
    </Tools>
</ToolsPartsMaterials>
<ItemizedList>
    <ListTitle>
        <Text>REFERENCED PROCEDURE(S):</Text>
    </ListTitle>
{% for line in referencedProcedures %}
    <Para>
        <Text>{{referencedProcedures}}</Text>
    </Para>
{% endfor %}
</ItemizedList>
